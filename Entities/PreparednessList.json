{
  "name": "PreparednessList",
  "type": "object",
  "properties": {
    "category": {
      "type": "string",
      "enum": [
        "home_kit",
        "car_kit",
        "emergency_plan",
        "documents",
        "contacts"
      ],
      "description": "Checklist category"
    },
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "checked": {
            "type": "boolean"
          },
          "notes": {
            "type": "string"
          }
        }
      },
      "description": "Checklist items"
    },
    "last_reviewed": {
      "type": "string",
      "format": "date",
      "description": "Last time checklist was reviewed"
    }
  },
  "required": []
}