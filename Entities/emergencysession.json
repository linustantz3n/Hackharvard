{
  "name": "EmergencySession",
  "type": "object",
  "properties": {
    "emergency_type": {
      "type": "string",
      "description": "Type of emergency (e.g., choking, chest pain, bleeding, etc.)"
    },
    "description": {
      "type": "string",
      "description": "User's description of the emergency"
    },
    "severity": {
      "type": "string",
      "enum": [
        "critical",
        "urgent",
        "moderate",
        "minor"
      ],
      "description": "AI-assessed severity level"
    },
    "instructions": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "step": {
            "type": "number"
          },
          "instruction": {
            "type": "string"
          },
          "critical": {
            "type": "boolean"
          }
        }
      },
      "description": "Step-by-step instructions provided"
    },
    "call_911_recommended": {
      "type": "boolean",
      "description": "Whether calling 911 was recommended"
    },
    "outcome": {
      "type": "string",
      "enum": [
        "helped",
        "called_911",
        "abandoned"
      ],
      "description": "Session outcome"
    }
  },
  "required": []
}